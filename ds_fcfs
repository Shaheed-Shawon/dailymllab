input_string = input("Enter integers separated by spaces: ")
list = list(map(int, input_string.split()))

head=60

sum=0
pointer=60

cal=abs(list[0]-head)
print(cal)
sum+=cal

for i in range(1,5):
        print(list[i-1],"-",list[i])
        cal=abs(list[i-1]-list[i])
        print(cal)
        sum+=cal
        
        
print(sum)    
        
